{
  "name": "@mikematos84/emotionless",
  "description": "The EmotionLess Design System",
  "main": "dist/cjs/index.js",
  "module": "dist/es/index.js",
  "version": "1.1.0",
  "license": "MIT",
  "sideEffects": false,
  "prettier": "./prettier.config.js",
  "files": [
    "dist/"
  ],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mikematos84/monorepo",
    "directory": "packages/emotionless"
  },
  "scripts": {
    "build:storybook": "storybook build -c .storybook -o out",
    "build:swcrc": "node ./scripts/build-swcrc.js",
    "format": "prettier src/**/*.{json,md,html,css,svg,js,jsx} --config ./prettier.config.js --ignore-path ./.prettierignore --write",
    "lint": "yarn build:swcrc && eslint src/**/*.{js,jsx} --fix",
    "prepare": "yarn build:swcrc && rollup -c",
    "storybook": "storybook dev -p 6006",
    "test": "yarn build:swcrc && jest",
    "watch": "rollup -c -w --no-treeshake --watch.onEnd='npx yalc push --scripts' --watch.exclude='dist/**'"
  },
  "devDependencies": {
    "@babel/core": "^7.19.1",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.18.6",
