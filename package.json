    "rollup-plugin-swc3": "^0.8.1",
    "storybook": "^7.6.3",
    "storybook-addon-mock": "^4.3.0",
    "storybook-addon-performance": "^0.17.1",
    "storybook-addon-swc": "^1.2.0",
    "storybook-dark-mode": "^3.0.3",
    "webpack": "^5.89.0"
  },
  "peerDependencies": {
    "@emotion/react": ">=11",
    "@emotion/styled": ">=11",
    "react": ">=16 <19",
    "react-dom": ">=16 <19"
  },
  "dependencies": {
    "@emotion-icons/material": "^3.14.0",
    "@emotion-icons/simple-icons": "^3.27.0",
    "fast-glob": "^3.3.2",
    "fs-extra": "^11.2.0",
    "lodash": "^4.17.21",
    "lorem-ipsum": "2.0.8",
    "prop-types": "^15.8.1"
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "src/**/*.{json,md,html,css,svg}": [
      "prettier --write"
    ]
  }
